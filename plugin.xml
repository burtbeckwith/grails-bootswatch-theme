<plugin name='bootswatch-theme' version='1.0.BUILD-SNAPSHOT' grailsVersion='1.3 &gt; *'>
  <author>Cristian Olaru</author>
  <authorEmail>colaru@gmail.com</authorEmail>
  <title>Bootswatch Theme</title>
  <description>\
Provides a Plugin Platform Theme based on Twitter Bootstrap Bootswatch Themes
</description>
  <documentation>http://grails.org/plugin/bootswatch-theme</documentation>
  <type>BootswatchThemeGrailsPlugin</type>
  <resources>
    <resource>BootstrapThemeResources</resource>
  </resources>
  <repositories>
    <repository name='http://grails.org/plugins' url='http://grails.org/plugins' />
    <repository name='grailsCentral' url='http://plugins.grails.org' />
    <repository name='http://repo.grails.org/grails/plugins' url='http://repo.grails.org/grails/plugins/' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='grailsCore' url='http://svn.codehaus.org/grails/trunk/grails-plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
  </repositories>
  <dependencies />
  <plugins>
    <compile>
      <plugin group='org.grails.plugins' name='bootstrap-ui' version='1.0-SNAPSHOT' />
      <plugin group='org.grails.plugins' name='platform-ui' version='1.0.M1-SNAPSHOT' />
    </compile>
  </plugins>
  <runtimePluginRequirements />
  <behavior />
</plugin>